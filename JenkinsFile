pipeline {
  agent any
  parameters
  {
       string defaultValue: 'stage', name: 'Environment'
   }
  stages {
    stage('Full Flow 3') {
      parallel {
        stage('LCTR') {
          steps {
            catchError(buildResult: 'SUCCESS', stageResult: 'FAILURE')
            {
			    script
			    {
                  echo "${params.Environment}".toLowerCase()

               }
            }
        }

        stage('micro_core') {
          steps {
            catchError(buildResult: 'SUCCESS', stageResult: 'FAILURE')
			    {
                    echo 'micro_core'
                }
          }
        }

      }
    }
	}

   }
}
